<Project>
  <!-- Set the repository root into a variable -->
  <PropertyGroup>
    <SourceRoot>$(MSBuildThisFileDirectory)</SourceRoot>
  </PropertyGroup>
  <!-- Set common properties regarding assembly information and nuget packages -->
  <PropertyGroup>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)Open.snk</AssemblyOriginatorKeyFile>
    <AssemblyOriginatorPublicKey></AssemblyOriginatorPublicKey>
    <Authors>Stephen Welsh</Authors>
    <Owners>Stephen Welsh, UnifiedFX</Owners>
    <Title>Trustless Reverse Tunnel for YARP - WebSocket Relay between ASPNet Core pipelines</Title>
    <Description>Trustless Reverse Tunnel for YARP - WebSocket Relay between ASPNet Core pipelines</Description>
    <Copyright>Copyright Â© UnifiedFX Limited</Copyright>
    <RepositoryUrl>https://github.com/BoBiene/UFX.Relay</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/BoBiene/UFX.Relay</PackageProjectUrl>
    <PackageIcon>ReverseTunnel.Yarp.Logo.png</PackageIcon>
    <PackageTags>Relay Tunnel YARP ReverseTunnel C# .NET</PackageTags>
    <PackageReadme>README.md</PackageReadme>
  </PropertyGroup>
  <!-- Common compile parameters -->
  <PropertyGroup>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>$(NoWarn);1701;1702;1705;1591;4014;3101;VSTHRD100</NoWarn>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <LangVersion>latest</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsPackable>false</IsPackable>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <None Include="$(SourceRoot)ReverseTunnel.Yarp.Logo.png" Pack="true" Visible="false" PackagePath="/" />
    <None Include="$(SourceRoot)README.md" Pack="true" Visible="false" PackagePath="/" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Nerdbank.GitVersioning" Condition="!Exists('packages.config')">
      <PrivateAssets>all</PrivateAssets>
      <Version>3.7.115</Version>
    </PackageReference>
  </ItemGroup>
</Project>